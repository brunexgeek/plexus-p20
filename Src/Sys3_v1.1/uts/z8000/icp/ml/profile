INCRT = $(ROOT)/usr/include
CFLAGS = -O -DICP -Dpdp11 -DPROFILE -I$(INCRT)
FRC =

FILES =\
	clist.o\
	cinit.o

all:	$(FILES)

clist.o:
	/lib/cpp -DASSM -DICP -Dpdp11 -I$(INCRT) -P  $< >tempfile
	as -u tempfile -o $@
	rm tempfile
	$(FRC)

cinit.o:\
	$(INCRT)/sys/param.h\
	$(INCRT)/sys/tty.h
	cc $(CFLAGS) -c $<
	$(FRC)

FRC:
